<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="JavaAutomation" verbose="1" parallel="methods" thread-count="20">
  <parameter name="env" value="local"/>
  <test name="Smoke">
    <classes>
      <class name="com.testsuite.tests.SmokeTests"/>
    </classes>
  </test>
  <test name="User Interface" parallel="methods" thread-count="20" data-provider-thread-count="10">
    <classes>
      <class name="com.testsuite.tests.UserInterfaceTests"/>
    </classes>
  </test>
  <test name="S3 Tests">
    <classes>
      <class name="com.testsuite.tests.S3Tests"/>
    </classes>
  </test>
</suite>